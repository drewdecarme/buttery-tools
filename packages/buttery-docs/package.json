{
  "name": "@buttery/docs",
  "version": "0.2.8",
  "description": "TODO",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn tsx ./scripts/build-icons-and-tokens.ts && yarn typechecks && yarn lib:build && yarn cli-scripts:build",
    "typechecks": "tsc --project tsconfig.typechecks.json",
    "cli-scripts:build": "tsc --project tsconfig.cli-scripts.build.json",
    "cli-scripts:dev": "yarn cli-scripts:build --watch",
    "lib:build": "tsx ./scripts/lib.build.ts && yarn lib:types",
    "lib:types": "tsc --project tsconfig.library.types.json",
    "lib:dev": "yarn lib:build --watch",
    "local:dev": "tsx ./scripts/local.dev.ts",
    "local:build": "tsx ./scripts/local.build.ts",
    "local:add": "tsx ./scripts/local.add.ts",
    "tokens:studio": "tsx ./scripts/tokens.studio.ts",
    "tokens:build": "tsx ./scripts/tokens.build.ts",
    "icons:build": "tsx ./scripts/icons.build.ts",
    "pages:deploy": "cd ./.buttery/docs/dist && wrangler pages deploy ./build/client --project-name buttery-docs-cf",
    "pages:dev": "cd ./.buttery/docs/dist && wrangler pages dev ./build/client",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "templates": "tsx ./scripts/templates/script.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/drewdecarme/buttery-tools.git",
    "directory": "packages/buttery-docs"
  },
  "files": [
    "dist",
    "app",
    "templates"
  ],
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drewdecarme/buttery-tools/issues"
  },
  "homepage": "https://github.com/drewdecarme/buttery-tools#readme",
  "exports": {
    "./cli/*": "./dist/cli-scripts/*.js",
    "./server": "./dist/lib/server/index.js",
    "./server.dev": "./dist/lib/server.dev/index.js",
    "./server.cloudflare-pages": "./dist/lib/server.cloudflare-pages/index.js",
    "./client": "./dist/lib/client/index.js",
    "./plugin-interactive-preview": "./dist/lib/plugin-interactive-preview/vite/index.js",
    "./plugin-interactive-preview/ui": "./dist/lib/plugin-interactive-preview/ui/index.js",
    ".": "./dist/lib/index.js",
    "./app": "./dist/lib/app/index.js",
    "./css": "./dist/lib/style.css"
  },
  "devDependencies": {
    "@buttery/icons": "workspace:*",
    "@buttery/tokens": "workspace:*",
    "@cloudflare/workers-types": "4.20250109.0",
    "@storybook/react": "8.5.1",
    "@types/babel__core": "7.20.5",
    "@types/express": "5.0.0",
    "@types/node": "22.10.9",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@wyw-in-js/vite": "0.5.5",
    "esbuild": "0.24.2",
    "rehype-parse": "9.0.1",
    "rehype-remark": "10.0.0",
    "remark-parse": "11.0.0",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "unified": "11.0.5",
    "unist-util-visit": "5.0.0",
    "vite": "6.0.11"
  },
  "dependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.26.0",
    "@buttery/components": "workspace:*",
    "@buttery/core": "workspace:*",
    "@buttery/logs": "workspace:*",
    "@buttery/meta": "workspace:0.1.6",
    "@buttery/utils": "workspace:*",
    "@inquirer/prompts": "7.2.1",
    "@linaria/core": "6.2.0",
    "@linaria/react": "6.2.1",
    "@linaria/server": "6.2.0",
    "@mdx-js/rollup": "3.1.0",
    "@shikijs/rehype": "1.26.1",
    "@stefanprobst/rehype-extract-toc": "2.2.1",
    "@vitejs/plugin-react": "4.3.4",
    "express": "4.21.2",
    "gray-matter": "4.0.3",
    "immer": "10.1.1",
    "open": "10.1.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-helmet-async": "2.0.5",
    "rehype-autolink-headings": "7.1.0",
    "rehype-slug": "6.0.0",
    "remark-frontmatter": "5.0.0",
    "remark-gfm": "4.0.0",
    "remark-mdx-frontmatter": "5.0.0",
