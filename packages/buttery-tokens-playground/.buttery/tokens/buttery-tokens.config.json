{
  "$schema": "../../../buttery-tokens-utils/schema.json",
  "runtime": {
    "namespace": "replace-me",
    "prefix": "--buttery",
    "strict": true,
    "suppressStrictWarnings": false
  },
  "size": {
    "documentFontSize": 16,
    "space": {
      "mode": "auto",
      "variants": 11
    }
  },
  "font": {
    "families": {},
    "fallback": "system-ui, \"Segoe UI\", Roboto, Helvetica, Arial, sans-serif, \"Apple Color Emoji\", \"Segoe UI Emoji\", \"Segoe UI Symbol\"",
    "weights": {},
    "variants": {}
  },
  "breakpoints": {
    "phone": 375,
    "tablet": 768,
    "desktop": 1280
  },
  "color": {
    "brand": {
      "type": "manual",
      "colors": {
        "brand2": {
          "hex": "#519550",
          "variants": {
            "light": "#b8ffb9",
            "dark": "#525252"
          }
        },
        "brand3": {
          "hex": "#000000",
          "variants": 10
        }
      }
    },
    "neutral": {
      "neutral": {
        "hex": "#384ebc",
        "variants": 10
      },
      "surface": {
        "hex": "#FAFAFA",
        "variants": 1
      },
      "background": {
        "hex": "#F5F5F5",
        "variants": 1
      }
    }
  },
  "custom": {}
}
