{
  "name": "@buttery/tsconfig",
  "version": "0.1.4",
  "description": "A collection of TypeScript configuration files for various types of buttery TS projects",
  "main": "./tsconfig.json",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/drewdecarme/buttery-tools.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./library": {
      "default": "./tsconfig.library.json",
      "import": "./tsconfig.library.json",
      "require": "./tsconfig.library.json",
      "node": "./tsconfig.library.json"
    }
  },
  "author": "Drew DeCarme (drewdecarme@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drewdecarme/buttery-tools/issues"
  },
  "homepage": "https://github.com/drewdecarme/buttery-tools#readme",
  "devDependencies": {
    "typescript": "5.7.3"
  }
}
